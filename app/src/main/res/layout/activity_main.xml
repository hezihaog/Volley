<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/get_request_http_url_connection_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="URLConnection GET 请求" />

    <Button
        android:id="@+id/get_request_http_client_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="HttpClient GET 请求" />

    <Button
        android:id="@+id/get_request_okhttp_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="OkHttp GET 请求" />

    <Button
        android:id="@+id/get_request_async_http_client_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="AsyncHttpClient GET 请求" />

    <Button
        android:id="@+id/get_request_go_http_client_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="GoHttpClient GET 请求" />
</LinearLayout>